generator client {
  binaryTargets   = ["native", "darwin-arm64"]
  provider = "prisma-client-js"
  previewFeatures = ["deno"]
  output          = "./generated/client"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model expansion {
  expansion_code String @id
  name           String
  num_cards      Int
}